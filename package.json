{
  "name": "code-awareness",
  "displayName": "Code Awareness",
  "description": "Low noise collaboration.",
  "icon": "media/icons/caw-logo.png",
  "version": "1.0.4",
  "publisher": "CodeAwareness",
  "author": "Mark Vasile",
  "repository": "https://github.com/CodeAwareness/cA.vscode",
  "bugs": "https://github.com/CodeAwareness/cA.vscode/issues",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "SCM Providers"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/web/extension.js",
  "browser": "./dist/web/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "caw.toggle",
        "title": "%extension.toggle%"
      },
      {
        "command": "caw.nextPeer",
        "title": "%extension.nextPeer%"
      },
      {
        "command": "caw.prevPeer",
        "title": "%extension.prevPeer%"
      },
      {
        "command": "caw.mergeSlice",
        "title": "%extension.mergeSlice%"
      },
      {
        "command": "caw.mergeAll",
        "title": "%extension.mergeAll%"
      },
      {
        "command": "caw.openPeerFile",
        "title": "%extension.openPeerFile%"
      },
      {
        "command": "caw.openDiff",
        "title": "%extension.openDiff%"
      },
      {
        "command": "caw.refresh",
        "title": "%extension.refresh%"
      },
      {
        "command": "caw.selectRange",
        "title": "%extension.selectRange%"
      },
      {
        "command": "caw.openFile",
        "title": "%extension.openFile%"
      }
    ],
    "keybindings": [
      {
        "command": "caw.toggle",
        "key": "ctrl+alt+p",
        "mac": "cmd+alt+p"
      },
      {
        "command": "caw.highlight",
        "key": "shift+f12",
        "mac": "shift+f12"
      },
      {
        "command": "caw.nextPeer",
        "key": "ctrl+alt+]",
        "mac": "cmd+alt+]"
      },
      {
        "command": "caw.prevPeer",
        "key": "ctrl+alt+[",
        "mac": "cmd+alt+["
      },
      {
        "command": "caw.mergeSlice",
        "key": "ctrl+alt+m",
        "mac": "cmd+alt+m"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "codeAwareness",
          "name": "Peer Diffs",
          "icon": "media/code.svg",
          "contextualTitle": "Files changed at peers.",
          "when": "workbenchState != empty"
        }
      ]
    },
    "menus": {
      "commandPalette": [
        {
          "command": "caw.toggle",
          "when": "config.caw.enabled"
        },
        {
          "command": "caw.nextPeer",
          "when": "config.caw.enabled"
        },
        {
          "command": "caw.prevPeer",
          "when": "config.caw.enabled"
        },
        {
          "command": "caw.mergeAll",
          "when": "config.caw.enabled"
        },
        {
          "command": "caw.mergeSlice",
          "when": "config.caw.enabled"
        },
        {
          "command": "caw.selectRange",
          "when": "config.caw.enabled"
        }
      ],
      "scm/resourceGroup/context": [
        {
          "command": "caw.mergeAll",
          "when": "scmProvider == cΩ",
          "group": "1_modification"
        }
      ],
      "scm/resourceFolder/context": [
        {
          "command": "caw.mergeAll",
          "when": "scmProvider == cΩ",
          "group": "inline"
        }
      ],
      "scm/resourceState/context": [
        {
          "command": "caw.openFile",
          "when": "scmProvider == cΩ",
          "group": "navigation"
        },
        {
          "command": "caw.openDiff",
          "when": "scmProvider == cΩ",
          "group": "navigation"
        }
      ],
      "editor/title": [
        {
          "command": "caw.openFile",
          "when": "scmProvider == cΩ",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "command": "caw.selectRange",
          "when": "scmProvider == cΩ",
          "group": "2_cΩ@1"
        }
      ],
      "scm/change/title": [
        {
          "command": "caw.mergeSlice",
          "when": "scmProvider == cΩ"
        }
      ]
    },
    "configuration": {
      "title": "Code Awareness",
      "properties": {
        "codeAwareness.catalog": {
          "type": "string",
          "default": "catalog",
          "description": "%caw.config.catalog%",
          "scope": "application"
        }
      }
    }
  },
  "scripts": {
    "test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/web/test/suite/index.js",
    "pretest": "npm run compile-web",
    "vscode:prepublish": "npm run package-web",
    "package": "npm run package-web && vsce package",
    "compile-web": "webpack",
    "package-web": "./prepare-prod.sh ; webpack --mode production --devtool hidden-source-map",
    "watch-web": "webpack --watch",
    "lint": "eslint src --ext ts",
    "run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
  },
  "devDependencies": {
    "@types/jest": "^29.2.2",
    "@types/lodash": "^4.14.188",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.9",
    "@types/vscode": "^1.72.0",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vscode/test-web": "^0.0.32",
    "assert": "^2.0.0",
    "babel-jest": "^29.2.2",
    "eslint": "^8.27.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.4",
    "eslint-plugin-n": "^15.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^1.5.0",
    "eslint-watch": "^8.0.0",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "lint-staged": "^13.0.3",
    "mocha": "^10.1.0",
    "node-sass": "^7.0.3",
    "path": "^0.12.7",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@changesets/cli": "^2.26.2",
    "lodash": "^4.17.21",
    "process": "^0.11.10",
    "vscode-nls": "^5.2.0"
  }
}
